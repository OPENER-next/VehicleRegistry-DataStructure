        </SanitaryEquipment>

        <TicketingEquipment version="any" id="TicketingEquipment/id/1">
          <!-- DELFI 3070 -->
          <TicketMachines>true</TicketMachines>
          <NumberOfMachines>1</NumberOfMachines>
        </TicketingEquipment>

        <!-- DELFI 3120 - Proposal: https://github.com/NeTEx-CEN/NeTEx/issues/873 -->
        <RampVehicleEquipment version="any" id="RampVehicleEquipment/id/1">
          <!-- DELFI 3123 -->
          <BearingCapacity>350</BearingCapacity>
          <!-- DELFI 3121 + calculation of DELFI 3127 via FloorHeight and PlatformHeight from QUAY -->
          <Length>1.685</Length>
          <!-- calculation of DELFI 3126 -->
          <ExternalLength>1.685</ExternalLength>
          <!-- DELFI 3122 + calculation of DELFI 3125 -->
          <ExternalWidth>0.817</ExternalWidth>
        </RampVehicleEquipment>

        <!-- DELFI 3130 - Proposal: https://github.com/NeTEx-CEN/NeTEx/issues/873 -->
        <HoistVehicleEquipment version="any" id="HoistVehicleEquipment/id/1">
          <!-- DELFI 3133 -->
          <BearingCapacity>350</BearingCapacity>
          <!-- calculation of DELFI 3132 -->
          <ExternalLength>1.685</ExternalLength>
          <!-- calculation of DELFI 3131 -->
          <ExternalWidth>0.817</ExternalWidth>
        </HoistVehicleEquipment>

        <AccessVehicleEquipment version="any" id="AccessVehicleEquipment/id/1">
          <Name>Entrance dimensions</Name>
          <!-- DELFI 3110 + DELFI 3112 -->
          <NumberOfSteps>1</NumberOfSteps>
          <!-- DELFI 3111 - Proposal: https://github.com/NeTEx-CEN/NeTEx/issues/900 -->
          <StepHeight>0.2</StepHeight>
          <!-- DELFI 3101 -->
          <BoardingHeight>0.4</BoardingHeight>
          <!-- DELFI 3100 - Proposal: https://github.com/NeTEx-CEN/NeTEx/issues/900 -->
          <FloorHeight>0.6</FloorHeight>
          <!-- DELFI 3090 + calculation of DELFI 3080 via EdgeToTrackCenterDistance from QUAY -->
          <!-- Proposal: https://github.com/NeTEx-CEN/NeTEx/issues/900 -->
          <EdgeToTrackCenterDistance>1.1</EdgeToTrackCenterDistance>
          <!-- DELFI 3041 -->
          <WidthOfAccessArea>1.3</WidthOfAccessArea>
          <HeightOfAccessArea>2</HeightOfAccessArea>
          <!-- DELFI 3040 (only partially) -->
          <AutomaticDoors>true</AutomaticDoors>
        </AccessVehicleEquipment>

        <!-- DELFI 3113 - Proposal: https://github.com/NeTEx-CEN/NeTEx/issues/873 -->
        <RetractableStepVehicleEquipment version="any" id="RetractableStepVehicleEquipment/id/1">
          <BoardingHeight>0.575</BoardingHeight>
          <ExternalLength>0.2</ExternalLength>
          <BearingCapacity>350</BearingCapacity>
